@article{Schork2001:Theor,
	Abstract = {The vibrational structures of the first photoelectron bands of the vinylidene anion and its deuterated isotopomers have been calculated theoretically, by an ab initio quantum dynamical method. Very good agreement with the experimental results of Lineberger and coworkers has been obtained. This implies a reproduction of the relevant vibrational frequencies to within 20-30 cm(-1) for all three neutral isotopomers. By a slight change of the anionic C-C bond length the agreement on the spectral intensities can be further improved. In this way an accurate estimate of the equilibrium geometry of the vinylidene anion has also been deduced.},
	Address = {CAMBRIDGE},
	Author = {Schork, R. and Koppel, H.},
